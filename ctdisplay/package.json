{
  "name": "CTDisplay",
  "displayName": "CT.Display",
  "description": "Plug-in that allows display continuous testing results from runner in Visual Studio Code. ",
  "version": "0.0.1",
  "publisher": "CT",
  "engines": {
    "vscode": "^1.5.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:ct.run"
  ],
  "main": "./out/src/extension",
  "contributes": {
    "commands": [
      {
        "command": "ct.run",
        "title": "CT: Start testing!"
      },
      {
        "command": "ctdisplay.updateTests",
        "title": "ctdisplay.updateTests"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "tsc -p ./",
    "compile": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install"
  },
  "devDependencies": {
    "typescript": "^2.0.3",
    "vscode": "^1.0.0",
    "mocha": "^2.3.3",
    "@types/node": "^6.0.40",
    "@types/mocha": "^2.2.32"
  },
  "dependencies": {
    "inversify": "^3.0.0-beta.2",
    "reflect-metadata": "^0.1.8"
  }
}
